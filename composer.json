{
  "name": "karellens/reactive-admin",
  "description": "Fast and easy customizable admin interface for Laravel 5",
  "type": "library",
  "keywords": ["administrator", "admin", "database", "laravel", "cms"],
  "homepage": "https://github.com/karellens/reactive-admin",
  "license": "MIT",
  "authors": [
    {
      "name": "Denis Bozhenkov",
      "email": "mastermind@karellens.com"
    }
  ],
  "support": {
    "issues": "https://github.com/karellens/reactive-admin/issues"
  },
  "require": {
    "php": ">=7.0.0",
    "laravel/framework": "~5.5",
    "intervention/image": "~2.3",
    "lavary/laravel-menu": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "laravel/laravel": "~5.5"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Karellens\\ReactiveAdmin\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Karellens\\ReactiveAdmin\\ReactiveAdminServiceProvider"
      ],
      "aliases": {
        "ReactiveAdmin": "Karellens\\ReactiveAdmin\\Facades\\ReactiveAdmin"
      }

    }
  },
  "minimum-stability": "dev"
}